def add(a, b):
    result = a + b
    print(result)
    if result > 20:
        message = "Big number"
        print(message)
    else:
        message = "Small number"
        print(message)
    for i in items:
        temp = i + result
        print(temp)
    while result < 100:
        result = result + 10


